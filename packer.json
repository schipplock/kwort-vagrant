{
    "builders": [{
        "type": "virtualbox-iso",
        "post_shutdown_delay": "30s",
        "boot_command": [
            "<enter><wait10><wait10><wait10>q",
            "dd if=/dev/zero of=/dev/sda bs=512 count=1 conv=notrunc<enter><wait>",
            "fdisk /dev/sda<enter><wait>",
            "n<enter><wait>",
            "p<enter><wait>",
            "1<enter><wait>",
            "2048<enter><wait>",
            "+18000M<enter><wait>",
            "w<enter><wait>",
            "fdisk /dev/sda<enter><wait>",
            "n<enter><wait>",
            "p<enter><wait>",
            "2<enter><wait>",
            "<enter><wait><wait>",
            "<enter><wait>",
            "t<enter><wait>",
            "2<enter><wait>",
            "82<enter><wait>",
            "w<enter><wait>",
            "mkfs.ext4 /dev/sda1<enter><wait>",
            "mkswap /dev/sda2<enter><wait>",
            "mount -t ext4 /dev/sda1 /mnt/install<enter><wait>",
            "pkgsinstall<enter><wait10><wait10><wait10><wait10><wait10><wait10>",
            "jumpOS<enter><wait>",
            "echo '/dev/sda1 / ext4 defaults' >> /etc/fstab<enter><wait>",
            "echo '/dev/sda2 swap swap defaults' >> /etc/fstab<enter><wait>",
            "> /etc/rc.conf<enter><wait>",
            "echo 'KEYMAP=qwertz/de-latin1' >> /etc/rc.conf<enter><wait>",
            "echo 'TIMEZONE=Europe/Berlin' >> /etc/rc.conf<enter><wait>",
            "echo 'HOSTNAME=kwort' >> /etc/rc.conf<enter><wait>",
            "echo 'MODULES=(tun)' >> /etc/rc.conf<enter><wait>",
            "echo 'LVM=no' >> /etc/rc.conf<enter><wait>",
            "echo 'SERVICES=(sysklogd lo net sshd)' >> /etc/rc.conf<enter><wait>",
            "kpkg install /root/bootloaders/lilo#24.2#x86_64#1.tar.xz<enter><wait>",
            "> /etc/lilo.conf<enter><wait>",
            "echo 'lba32' >> /etc/lilo.conf<enter><wait>",
            "echo 'install=text' >> /etc/lilo.conf<enter><wait>",
            "echo 'boot=/dev/sda' >> /etc/lilo.conf<enter><wait>",
            "echo 'image=/boot/vmlinuz' >> /etc/lilo.conf<enter><wait>",
            "echo '    label=Kwort' >> /etc/lilo.conf<enter><wait>",
            "echo '    read-only' >> /etc/lilo.conf<enter><wait>",
            "echo '    append=\"root=/dev/sda1\"' >> /etc/lilo.conf<enter><wait>",
            "lilo<enter><wait>",
            "passwd<enter><wait>",
            "vagrant<enter><wait>vagrant<enter><wait>vagrant<enter><wait>",
            "useradd -m vagrant<enter><wait>",
            "chsh -s /bin/bash vagrant<enter><wait>",
            "passwd vagrant<enter><wait>",
            "vagrant<enter><wait>vagrant<enter><wait>vagrant<enter><wait>",
            "echo 'vagrant ALL=(ALL) NOPASSWD: ALL' >> /etc/sudoers<enter><wait>",
            "cd /home/vagrant<enter><wait>",
            "mkdir -p .ssh<enter><wait>",
            "chmod 700 .ssh<enter><wait>",
            "cd .ssh<enter><wait>",
            "echo 'ssh-rsa  vagrant insecure public key' > authorized_keys<enter><wait>",
            "chmod 600 authorized_keys<enter><wait>",
            "chown -R vagrant /home/vagrant<enter><wait>",
            "reboot<enter>"
        ],
        "guest_os_type": "Linux_64",
        "guest_additions_mode": "disable",
        "iso_url": "kwort-4.3.2.iso",
        "iso_checksum": "6a5aaafd0f4d327fe7c2a944e2bd8415b67fc9fe4c20ed7a7853824c0892869d",
        "iso_checksum_type": "sha256",
        "disk_size": "20000",
        "ssh_username": "vagrant",
        "ssh_password": "vagrant",
        "shutdown_command": "echo 'vagrant' | sudo -S shutdown -h now",
        "shutdown_timeout": "999999m",
        "ssh_wait_timeout": "999999m",
        "vboxmanage": [
            ["modifyvm", "{{.Name}}", "--memory", "1024"],
            ["modifyvm", "{{.Name}}", "--cpus", "1"]
          ]
    }],
    "post-processors": [{
        "type": "vagrant",
        "compression_level": 9,
        "keep_input_artifact": false
    }]
}
